{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "mean-app"
  },
  "apps": [
    {
      "root": "assets",
      "outDir": "public",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.hbs",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "style.css",
        "jquery-ui-1.12.1.custom/jquery-ui.css",
        "jquery-ui-1.12.1.custom/jquery-ui.theme.css",
        "jquery-ui-1.12.1.custom/jquery-ui.theme.min.css",
        "jquery-ui-1.12.1.custom/jquery-ui.min.css",
        "jquery-ui-1.12.1.custom/jquery-ui.structure.css",
        "jquery-ui-1.12.1.custom/jquery-ui.structure.min.css",
        "../node_modules/jquery-datetimepicker/build/jquery.datetimepicker.min.css"
      ],
      "scripts": [
          "jquery-ui-1.12.1.custom/jquery-ui.js",
          "jquery-ui-1.12.1.custom/external/jquery/jquery.js",
          "jquery-ui-1.12.1.custom/jquery-ui.min.js",
           "../node_modules/jquery/dist/jquery.min.js",
           "../node_modules/jquery-datetimepicker/build/jquery.datetimepicker.full.min.js",
           "../node_modules/jquery-datetimepicker/build/jquery.datetimepicker.full.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "tsconfig.json"
    },
    {
      "project": "tsconfig.aot.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
